<Map>
  <Style name="color">
    <Rule>
        <RasterSymbolizer mode="normal" />
    </Rule>
  </Style>  
  <Style name="slope">
    <Rule>
      <RasterSymbolizer opacity="0.1" mode="multiply" />
    </Rule>
  </Style> 
  <Style name="hillshade">
    <Rule>
      <RasterSymbolizer opacity="0.4" mode="multiply" />
    </Rule>
  </Style>
  <Style name='water'>
    <Rule>
        <PolygonSymbolizer fill="steelblue"/>
    </Rule>
  </Style>
  <Style name='mask'>
    <Rule>
        <RasterSymbolizer/>
    </Rule>
</Style>
